{
	// 监听端口
	"containerPort": 808080808,
	// Dockerfile 路径
	"dockerfilePath": "Dockerfile",
	// 构建目录
	"buildDir": "",
	// 最小实例数
	"minNum": 0,
	// 最大实例数
	"maxNum": 4,
	// cpu 核数
	"cpu": 1,
	// 内存大小，单位：G
	"mem": 2,
	// 扩缩容指标类型
	"policyType": "cpu",
	// 扩缩容指标阈值
	"policyThreshold": 90,
	// 环境变量
	"envParams": {},
	// 日志采集路径
	"customLogs": "stdout",
	// 启动检测延迟
	"initialDelaySeconds": 2
}
